{
  "name": "@wps/icalink-sync",
  "version": "0.0.1",
  "private": true,
  "description": "基于Stratix框架的模块化项目模板",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "env": "stratix encrypt prod.env.json",
    "env:dev": "stratix encrypt dev.env.json",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@stratix/core": "workspace:*",
    "@stratix/utils": "workspace:*",
    "@stratix/web": "workspace:*",
    "@stratix/database": "workspace:*",
    "@stratix/was-v7": "workspace:*",
    "mysql2": "^3.14.1",
    "jsonwebtoken": "^9.0.2",
    "awilix": "^12.0.5",
    "@stratix/queue": "workspace:*",
    "@stratix/tasks": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "tsx": "^4.19.4",
    "typescript": "5.8.3",
    "vitest": "^3.1.2",
    "@types/jsonwebtoken": "^9.0.9"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
