{
  "name": "@stratix/gateway",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc",
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "env": "stratix config encrypt prod.env.json",
    "env:dev": "stratix encrypt dev.env.json",
    "stratix": "stratix",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@stratix/core": "workspace:*",
    "@stratix/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "tsx": "^4.19.4",
    "typescript": "5.8.3",
    "vitest": "^3.1.2",
    "@types/jsonwebtoken": "^9.0.9"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
