# Vite环境变量 (必须以VITE_开头)

# 应用配置
VITE_APP_TITLE=AgendaEdu Web
VITE_APP_VERSION=0.0.1
VITE_APP_DESCRIPTION=教育日程管理系统

# API配置
VITE_API_URL=https://chat.whzhsc.cn/api
VITE_API_TIMEOUT=10000

# 认证配置
VITE_AUTH_ENABLED=true
VITE_JWT_STORAGE_KEY=agendaedu_token

# 功能开关
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=false

# 主题配置
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true

# 分页配置
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=100

# 文件上传配置
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# 外部服务
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# 开发配置
VITE_MOCK_DELAY=1000
VITE_SHOW_DEV_TOOLS=true 