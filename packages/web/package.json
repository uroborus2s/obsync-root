{
  "name": "@stratix/web",
  "version": "1.0.0",
  "description": "轻量级基于Koa的HTTP服务框架",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.es6.json",
    "build:cjs": "tsc -p tsconfig.node.json",
    "build": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs",
    "dev": "tsc -p tsconfig.es6.json --watch"
  },
  "exports": {
    "development": "./src/index.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.5.0",
    "@fastify/swagger-ui": "^5.2.2",
    "@stratix/utils": "workspace:*",
    "fastify": "^5.2.1",
    "json-schema-to-ts": "^3.1.1",
    "path-to-regexp": "^8.2.0"
  },
  "peerDependenciesMeta": {}
}
