{
  "name": "@stratix/queue",
  "version": "1.0.0-beta.4",
  "description": "Stratix Queue Plugin based on BullMQ with adapter pattern, dependency injection and cross-plugin sharing",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc",
    "build:watch": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "stratix",
    "queue",
    "bullmq",
    "adapter",
    "dependency-injection",
    "typescript",
    "plugin"
  ],
  "author": "Stratix Team",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "bullmq": "3.18.0",
    "@stratix/utils": "workspace:*"
  },
  "peerDependencies": {
    "@stratix/core": "workspace:*",
    "@stratix/redis": "workspace:*"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=22.0.0"
  }
}
