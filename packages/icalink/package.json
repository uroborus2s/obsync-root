{
  "name": "@stratix/icalink",
  "version": "1.0.0",
  "description": "Stratix framework plugin for syncing school schedule data to WPS Collaboration calendar",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "stratix",
    "plugin",
    "schedule",
    "calendar",
    "wps",
    "education",
    "sync"
  ],
  "author": "Stratix Team",
  "license": "MIT",
  "dependencies": {
    "@stratix/utils": "workspace:*",
    "cron": "^3.1.6",
    "jsonwebtoken": "^9.0.2",
    "awilix": "^12.0.5"
  },
  "devDependencies": {
    "@types/cron": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.9"
  },
  "peerDependencies": {
    "@stratix/core": "workspace:*",
    "@stratix/database": "workspace:*",
    "@stratix/was-v7": "workspace:*",
    "@stratix/tasks": "workspace:*",
    "@stratix/queue": "workspace:*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
