{
  "name": "@stratix/utils",
  "version": "0.0.1-beta.0",
  "description": "Stratix framework common utility functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc",
    "lint": "eslint src --ext .ts",
    "dev": "tsc --watch",
    "build:docs": "api-extractor run --local --verbose",
    "build:api-docs": "api-documenter markdown -i api/temp -o ../../apps/api-website/docs/api/utils && rimraf api/temp",
    "docs:generate": "pnpm run build && pnpm run build:docs && pnpm run build:api-docs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./common": {
      "import": "./dist/common/index.js",
      "types": "./dist/types/common/index.d.ts"
    },
    "./async": {
      "import": "./dist/async/index.js",
      "types": "./dist/types/async/index.d.ts"
    },
    "./string": {
      "import": "./dist/string/index.js",
      "types": "./dist/types/string/index.d.ts"
    },
    "./data": {
      "import": "./dist/data/index.js",
      "types": "./dist/types/data/index.d.ts"
    },
    "./number": {
      "import": "./dist/number/index.js",
      "types": "./dist/types/number/index.d.ts"
    },
    "./time": {
      "import": "./dist/time/index.js",
      "types": "./dist/types/time/index.d.ts"
    },
    "./environment": {
      "import": "./dist/environment/index.js",
      "types": "./dist/types/environment/index.d.ts"
    },
    "./performance": {
      "import": "./dist/performance/index.js",
      "types": "./dist/types/performance/index.d.ts"
    },
    "./context": {
      "import": "./dist/context/index.js",
      "types": "./dist/types/context/index.d.ts"
    }
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "glob": "^11.0.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/uuid": "^9.0.8"
  },
  "keywords": [
    "stratix",
    "utils",
    "utility",
    "functions"
  ],
  "author": "Stratix Team",
  "license": "MIT"
}
